  {{!< default}}
  <!-- Author -->
  <div class="row blue text-center" itemscope itemtype="http://schema.org/Article">
    <div class="columns" itemprop="article">
      {{#author}}
        <h1>{{name}}</h1>
        {{#if bio}}<p>{{bio}}</p>{{/if}}
      {{/author}}
    </div>
  </div>
  {{> "loop"}}
  <!-- RSS -->
  <div class="row">
    <div class="columns">
      <p class="text-center"><a href="{{@blog.url}}{{#author}}{{url}}{{/author}}rss/" target="_blank" rel="noopener"><img src="https://www.builtbygoat.com/assets/images/rss.svg" height="24" width="24" alt="RSS Feed"></a></p>
    </div>
  </div>