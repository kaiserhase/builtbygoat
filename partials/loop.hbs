  {{#foreach posts}}
  <div class="large-12 columns loop">
      <div class="article-title">
        <p class="post-unify2"><span class="date"><amp-img src="https://www.builtbygoat.com/images/date.svg" height="16" width="16" alt="Date Posted"></amp-img> Date</span> <strong><time datetime="{{date format='YYYY-MM-DD'}}">{{date format="DD MMMM YYYY"}}</time></strong></p>
        <p class="article-banner {{#if feature_image}}banner-height{{/if}}">{{#if feature_image}}<amp-img src="{{feature_image}}" height="645px" width="968px" layout="responsive" alt="Article Header"></amp-img>{{/if}}</p>
        <h2><a href="{{url}}">{{title}}</a></h2>
        <p class="post-unify"><span class="author"><amp-img src="https://www.builtbygoat.com/images/author.svg" height="16" width="16" alt="Posted By"></amp-img> Author</span> <strong>{{author}}</strong></p>
      </div>
      <div class="article-detail">
        <p class="post-unify">{{excerpt}} <strong><a href="{{url}}">&rarr;READ REST OF ARTICLE</a></strong></p>
      </div>
      <div class="article-byline">
          <p class="subpost post-unify"><span class="share"><amp-img src="https://www.builtbygoat.com/images/share.svg" height="16" width="16" alt="Share This Post"></amp-img></span> <strong><span class="twitter"><a href="https://twitter.com/intent/tweet?url={{url absolute="true"}}" target="_blank" rel="noopener">Twitter</a></span> <span class="facebook"><a href="https://www.facebook.com/sharer/sharer.php?u={{url absolute="true"}}" target="_blank" rel="noopener">Facebook</a></span> <span class="linkedin"><a href="http://www.linkedin.com/shareArticle?mini=true&url={{url absolute="true"}}" target="_blank" rel="noopener">LinkedIn</a></span></strong></p>
          <p class="subpost post-unify"><span class="tags"><amp-img src="https://www.builtbygoat.com/images/tags.svg" height="16" width="16" alt="Posted Tagged Under"></amp-img></span> <strong>{{#if tags}}{{tags}}{{else}}Uncategorized{{/if}}</strong></p>
        </div>
      
    </div>
  {{/foreach}}